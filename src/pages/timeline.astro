</MainLayout>

<style>
    .timeline {
  @apply relative before:absolute before:bg-gray-200 before:top-0 before:bottom-0 before:w-1 before:left-1/2 before:transform before:-translate-x-1/2;
}

.timeline-snap-icon {
  @apply flex flex-col items-center;
}

.max-md:timeline-compact {
  @apply md:flex-row md:items-center;
}

.timeline-vertical {
  @apply flex flex-col;
}

.timeline-middle {
  @apply relative flex items-center justify-center;
}

.timeline-start,
.timeline-end {
  @apply relative flex flex-col items-center md:items-end text-center md:text-left mb-10 md:mb-0;
}

.timeline-start .font-black,
.timeline-end .font-black {
  @apply text-lg font-bold;
}

hr {
  @apply border-t border-gray-200 my-4 w-full;
}

svg {
  @apply h-5 w-5 text-gray-400;
}

</style>