---
import { buttonVariants } from "@/components/ui/button";
import { siteConfig } from "@/config/site";
import MainLayout from "@/layouts/main-layout.astro";
import { cn } from "@/lib/utils";
import { Icon } from "astro-icon/components";
import Features from "@/sections/features.astro";
import FaqSection from "@/components/faq-section";
import { ThemeToggle } from "@/components/theme-toggle";

---

<MainLayout title="ONE">
    <div
      class="container flex max-w-[64rem] flex-col items-center gap-2 text-center"
    >
     
     
    <ThemeToggle client:idle />


   <h1
        class="font-heading text-4xl font-bold sm:text-5xl md:text-6xl lg:text-7xl !leading-[1.1] text-balance"
      >
        <span class="text-gradient_indigo-purple">
          Own <span class="font-sans font-bold">Your</span> Agents<span
            class="font-sans font-bold"></span>
        </span>
<h2>
  Unleash a swarm of AI agents to do important jobs in your enterprise or life. 
</h2>

      Your keys. Your data. Your code. Your Brand. Free. Open. Forever. Compliant.
      Profitable. MIT Licensed.
<br><br>
      <p
        class="max-w-[42rem] leading-normal text-muted-foreground sm:text-xl sm:leading-8 text-balance"
      >
      </p>
      </div>
      <div
        class="mx-auto grid justify-center gap-4 sm:grid-cols-2 md:max-w-[64rem] md:grid-cols-3"
      >
      <div
          class="relative overflow-hidden rounded-lg border bg-background p-2"
        >
          <div class="flex h-[180px] flex-col justify-between rounded-md p-5">
            <svg
              xmlns="http://www.w3.org/2000/svg"
              width="50"
              height="50"
              viewBox="0 0 24 24"
              fill="none"
              stroke="currentColor"
              stroke-width="2"
              stroke-linecap="round"
              stroke-linejoin="round"
              class="tabler-icon tabler-icon-user"
              ><path d="M8 7a4 4 0 1 0 8 0a4 4 0 0 0 -8 0"></path><path
                d="M6 21v-2a4 4 0 0 1 4 -4h4a4 4 0 0 1 4 4v2"></path>
            </svg>
            <h3 class="font-bold">Your Identity</h3>
            <p class="text-sm text-muted-foreground">
              This is your custom AI connected to your identity. Designed for world leaders, their children and everyone in-between.  
            </p>
          </div>
        </div>

        <div
          class="relative overflow-hidden rounded-lg border bg-background p-2"
        >
          <div class="flex h-[220px] flex-col justify-between rounded-md p-5">
            <svg
              xmlns="http://www.w3.org/2000/svg"
              width="50"
              height="50"
              viewBox="0 0 24 24"
              fill="none"
              stroke="currentColor"
              stroke-width="2"
              stroke-linecap="round"
              stroke-linejoin="round"
              class="tabler-icon tabler-icon-sparkles"
              ><path
                d="M16 18a2 2 0 0 1 2 2a2 2 0 0 1 2 -2a2 2 0 0 1 -2 -2a2 2 0 0 1 -2 2zm0 -12a2 2 0 0 1 2 2a2 2 0 0 1 2 -2a2 2 0 0 1 -2 -2a2 2 0 0 1 -2 2zm-7 12a6 6 0 0 1 6 -6a6 6 0 0 1 -6 -6a6 6 0 0 1 -6 6a6 6 0 0 1 6 6z"
              ></path>
            </svg>


            <h3 class="font-bold">Your Models</h3>
            <p class="text-sm text-muted-foreground">
              Use any AI model at cost price with your own API keys. Open AI, Google, Antrophic, Open Router. 
            </p>
          </div>
        </div>

        <div
          class="relative overflow-hidden rounded-lg border bg-background p-2"
        >
          <div class="flex h-[220px] flex-col justify-between rounded-md p-5">
            <svg
              xmlns="http://www.w3.org/2000/svg"
              width="50"
              height="50"
              viewBox="0 0 24 24"
              fill="none"
              stroke="currentColor"
              stroke-width="2"
              stroke-linecap="round"
              stroke-linejoin="round"
              class="tabler-icon tabler-icon-message"
            >
              <path d="M8 9h8"></path>
              <path d="M8 13h6"></path>
              <path
                d="M18 4a3 3 0 0 1 3 3v8a3 3 0 0 1 -3 3h-5l-5 3v-3h-2a3 3 0 0 1 -3 -3v-8a3 3 0 0 1 3 -3h12z"
              ></path>
            </svg>

            <h3 class="font-bold">Your Chats</h3>
            <p class="text-sm text-muted-foreground">
              Organise your AI conversations neatly in folders.
            </p>
          </div>
        </div>
        
        <div
          class="relative overflow-hidden rounded-lg border bg-background p-2"
        >
          <div class="flex h-[180px] flex-col justify-between rounded-md p-5">
            <svg
              xmlns="http://www.w3.org/2000/svg"
              width="50"
              height="50"
              viewBox="0 0 24 24"
              fill="none"
              stroke="currentColor"
              stroke-width="2"
              stroke-linecap="round"
              stroke-linejoin="round"
              class="tabler-icon tabler-icon-pencil"
              ><path d="M4 20h4l10.5 -10.5a2.828 2.828 0 1 0 -4 -4l-10.5 10.5v4"
              ></path><path d="M13.5 6.5l4 4"></path>
            </svg>
            <h3 class="font-bold">Your Prompts</h3>
            <p class="text-sm text-muted-foreground">
              Organise your AI prompts and access them seamlesssly.
            </p>
          </div>
        </div>
        <div
          class="relative overflow-hidden rounded-lg border bg-background p-2"
        >
          <div class="flex h-[180px] flex-col justify-between rounded-md p-5">
            <svg
              xmlns="http://www.w3.org/2000/svg"
              width="50"
              height="50"
              viewBox="0 0 24 24"
              fill="none"
              stroke="currentColor"
              stroke-width="2"
              stroke-linecap="round"
              stroke-linejoin="round"
              class="mr-3"
              ><path d="M3 21l18 0"></path><path d="M9 8l1 0"></path><path
                d="M9 12l1 0"></path><path d="M9 16l1 0"></path><path
                d="M14 8l1 0"></path><path d="M14 12l1 0"></path><path
                d="M14 16l1 0"></path><path
                d="M5 21v-16a2 2 0 0 1 2 -2h10a2 2 0 0 1 2 2v16"></path>
            </svg>
            <h3 class="font-bold">Your Work Spaces</h3>
            <p class="text-sm text-muted-foreground">
              Create AI workspaces designed for different tasks.
            </p>
          </div>
        </div>
        <div
          class="relative overflow-hidden rounded-lg border bg-background p-2"
        >
          <div class="flex h-[180px] flex-col justify-between rounded-md p-5">
            <svg
              xmlns="http://www.w3.org/2000/svg"
              width="50"
              height="50"
              viewBox="0 0 24 24"
              fill="none"
              stroke="currentColor"
              stroke-width="2"
              stroke-linecap="round"
              stroke-linejoin="round"
              class="tabler-icon tabler-icon-file"
              ><path d="M14 3v4a1 1 0 0 0 1 1h4"></path><path
                d="M17 21h-10a2 2 0 0 1 -2 -2v-14a2 2 0 0 1 2 -2h7l5 5v11a2 2 0 0 1 -2 2z"
              ></path>
            </svg>
            <h3 class="font-bold">Your Files</h3>
            <p class="text-sm text-muted-foreground">
              Upload your documents and make them available to your AI.
            </p>
          </div>
        </div>
        <div
          class="relative overflow-hidden rounded-lg border bg-background p-2"
        >
          <div class="flex h-[180px] flex-col justify-between rounded-md p-5">
            <svg
              xmlns="http://www.w3.org/2000/svg"
              width="50"
              height="50"
              viewBox="0 0 24 24"
              fill="none"
              stroke="currentColor"
              stroke-width="1"
              stroke-linecap="round"
              stroke-linejoin="round"
              class="tabler-icon tabler-icon-books"
              ><path
                d="M5 4m0 1a1 1 0 0 1 1 -1h2a1 1 0 0 1 1 1v14a1 1 0 0 1 -1 1h-2a1 1 0 0 1 -1 -1z"
              ></path><path
                d="M9 4m0 1a1 1 0 0 1 1 -1h2a1 1 0 0 1 1 1v14a1 1 0 0 1 -1 1h-2a1 1 0 0 1 -1 -1z"
              ></path><path d="M5 8h4"></path><path d="M9 16h4"></path><path
                d="M13.803 4.56l2.184 -.53c.562 -.135 1.133 .19 1.282 .732l3.695 13.418a1.02 1.02 0 0 1 -.634 1.219l-.133 .041l-2.184 .53c-.562 .135 -1.133 -.19 -1.282 -.732l-3.695 -13.418a1.02 1.02 0 0 1 .634 -1.219l.133 -.041z"
              ></path><path d="M14 9l4 -1"></path><path d="M16 16l3.923 -.98"
              ></path>
            </svg>
            <h3 class="font-bold">Your Collections</h3>
            <p class="text-sm text-muted-foreground">
              Group documents into collections.
            </p>
          </div>
        </div>
        <div
          class="relative overflow-hidden rounded-lg border bg-background p-2"
        >
          <div class="flex h-[180px] flex-col justify-between rounded-md p-5">
            <svg
              xmlns="http://www.w3.org/2000/svg"
              width="50"
              height="50"
              viewBox="0 0 24 24"
              fill="none"
              stroke="currentColor"
              stroke-width="2"
              stroke-linecap="round"
              stroke-linejoin="round"
              class="tabler-icon tabler-icon-robot-face"
              ><path
                d="M6 5h12a2 2 0 0 1 2 2v12a2 2 0 0 1 -2 2h-12a2 2 0 0 1 -2 -2v-12a2 2 0 0 1 2 -2z"
              ></path><path d="M9 16c1 .667 2 1 3 1s2 -.333 3 -1"></path><path
                d="M9 7l-1 -4"></path><path d="M15 7l1 -4"></path><path
                d="M9 12v-1"></path><path d="M15 12v-1"></path>
            </svg>
            <h3 class="font-bold">Your Assistants</h3>
            <p class="text-sm text-muted-foreground">
              Create a digital workforce.
            </p>
          </div>
        </div>
        <div
          class="relative overflow-hidden rounded-lg border bg-background p-2"
        >
          <div class="flex h-[180px] flex-col justify-between rounded-md p-5">
            <svg
              xmlns="http://www.w3.org/2000/svg"
              width="50"
              height="50"
              viewBox="0 0 24 24"
              fill="none"
              stroke="currentColor"
              stroke-width="2"
              stroke-linecap="round"
              stroke-linejoin="round"
              class="tabler-icon tabler-icon-bolt"
              ><path d="M13 3l0 7l6 0l-8 11l0 -7l-6 0l8 -11"></path>
            </svg>
            <h3 class="font-bold">Your Tools</h3>
            <p class="text-sm text-muted-foreground">
              Give your AI access to use any software or service.
            </p>
          </div>
        </div>
      </div>
    </div>

    <div></div>
  </section>
</MainLayout>
