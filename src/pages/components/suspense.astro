---
import Layout from "@/layouts/main-layout.astro";
import Component from "@/blocks/HeroWavy.astro";
import { Suspense } from "simple-stack-stream/components";

type Component = {
  name: string;
  url: string;
  image: string;
};
---

<Layout title="ONE">
  <main>
    <Suspense ssr="false">
      <Component />
      <p slot="fallback">Loading...</p>
    </Suspense>
  </main>
</Layout>
