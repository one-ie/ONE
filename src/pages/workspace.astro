import Icon from "astro-icon";
import Hero from "@/blocks/HeroTest.astro";
import Cal from "@/blocks/Cal.astro";
import Channels from "@/blocks/Models.astro";
import Stats from "@/blocks/Stats.astro";
import Features from "@/blocks/FeaturesGrid.astro";
