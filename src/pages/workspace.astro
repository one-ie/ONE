<style>

  .carousel {
  overflow: hidden;
}

.carousel-item {
  transition: transform 1s ease-in-out;
}

.carousel-item > div {
  scroll-snap-align: start;
}

.carousel-item:first-child {
  animation: carousel 36s linear infinite;
}

@keyframes carousel {
  0% {
    transform: translateX(0);
  }
  100% {
    transform: translateX(calc(-100% + 100vw));
  }
}

.dark .bg-background {
